{
  "scopeName": "source.zzlang",
  "patterns": [
    {
      "name": "keyword.control.zzlang",
      "match": "\\b(fn|let|if|elif|else|return|extern)\\b"
    },
    {
      "name": "storage.type.zzlang",
      "match": "\\b(int32_t|uint64_t|float|double|boolean|none|string|uint32_t|uint16_t|uint8_t|int64_t|int16_t|size_t)\\b"
    },
    {
      "name": "support.function.call.zzlang",
      "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\b(?=\\s*\\()"
    },
    {
      "name": "variable.other.zzlang",
      "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\b"
    },
    {
      "name": "constant.numeric.zzlang",
      "match": "\\b\\d+\\b"
    },
    {
      "name": "string.quoted.double.zzlang",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "comment.line.double-slash.zzlang",
      "match": "//.*$"
    }
  ]
}
