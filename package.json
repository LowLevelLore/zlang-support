{
  "name": "zlang-support",
  "displayName": "Zlang support",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "zlang",
        "aliases": [
          "Zlang",
          "zlang"
        ],
        "extensions": [
          ".z"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "zlang",
        "scopeName": "source.zlang",
        "path": "./syntaxes/zlang.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "zlang",
        "path": "./snippets/zlang.code-snippets"
      }
    ]
  }
}
